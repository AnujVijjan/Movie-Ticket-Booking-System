    Private Sub LinkLabel1_LinkClicked(sender As Object, e As LinkLabelLinkClickedEventArgs) Handles LinkLabel1.LinkClicked
        If (Home.TextBox1.Text = "<-  You Are In Mumbai : Western") Then
            Movies_Theatre.Label1.Text = LinkLabel1.Text
            Movies_Theatre.Show()
        ElseIf (Home.TextBox1.Text = "<-  You Are In Mumbai : South And Central") Then
            South_And_Central_Theatre.Label1.Text = LinkLabel1.Text
            South_And_Central_Theatre.Show()
