            Ahmednagar_Theatre.Label1.Text = LinkLabel1.Text
            Ahmednagar_Theatre.Show()
        ElseIf (Home.TextBox1.Text = "<-  You Are In Delhi") Then
            Delhi_Theatre.Label1.Text = LinkLabel1.Text
            Delhi_Theatre.Show()
        ElseIf (Home.TextBox1.Text = "<-  You Are In Bengaluru") Then
            Bengaluru_Theatre.Label1.Text = LinkLabel1.Text
            Bengaluru_Theatre.Show()
        ElseIf (Home.TextBox1.Text = "<-  You Are In Pune") Then
            Pune_Theatre.Label1.Text = LinkLabel1.Text
            Pune_Theatre.Show()
        ElseIf (Home.TextBox1.Text = "<-  You Are In Kolkata") Then
            Kolkata_Theatre.Label1.Text = LinkLabel1.Text
            Kolkata_Theatre.Show()
        ElseIf (Home.TextBox1.Text = "<-  You Are In Chennai") Then
            Chennai_Theatre.Label1.Text = LinkLabel1.Text
            Chennai_Theatre.Show()
        ElseIf (Home.TextBox1.Text = "<-  You Are In Aurangabad") Then
            Aurangabad.Label1.Text = LinkLabel1.Text
            Aurangabad.Show()
        End If
        Me.Close()
    End Sub

